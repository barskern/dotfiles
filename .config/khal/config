[calendars]

[[homie_local]]
path = ~/.local/share/calendars/homie/*
type = discover

[[courses_local]]
path = ~/.local/share/calendars/courses/
type = calendar
readonly = True

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M
